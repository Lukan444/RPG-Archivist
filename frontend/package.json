    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "axe-core": "^4.10.3",
    "cypress": "^14.3.2",
    "cypress-axe": "^1.6.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-axe": "^10.0.0",
    "k6": "^0.0.0",
    "lighthouse": "^12.5.1",
    "lighthouse-ci": "^1.13.1",
    "pa11y": "^8.0.0",
    "pa11y-ci": "^3.1.0",
    "prettier": "^2.8.8",
    "puppeteer": "^24.7.2",
    "react-scripts": "5.0.1",
    "start-server-and-test": "^2.0.11",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:frontend": "react-scripts start",
    "start:backend": "cd ../backend && npm run start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress:open",
    "e2e:run": "start-server-and-test start http://localhost:3000 cypress:run",
    "perf:lighthouse": "node performance-tests/lighthouse/run-lighthouse.js",
    "perf:load": "k6 run performance-tests/k6/load-test.js",
    "perf:stress": "k6 run performance-tests/k6/stress-test.js",
    "perf:spike": "k6 run performance-tests/k6/spike-test.js",
    "perf:soak": "k6 run performance-tests/k6/soak-test.js",
    "perf:all": "node performance-tests/run-performance-tests.js",
    "a11y:pa11y": "node accessibility-tests/pa11y/run-pa11y.js",
    "a11y:pa11y-ci": "pa11y-ci --config accessibility-tests/pa11y/.pa11yci",
    "a11y:cypress": "start-server-and-test start http://localhost:3000 'cypress run --spec cypress/e2e/accessibility/accessibility.cy.ts'",
    "a11y:components": "react-scripts test --testMatch='**/*.a11y.test.{ts,tsx}'",
    "a11y:all": "npm run a11y:pa11y && npm run a11y:cypress && npm run a11y:components",
    "browser:chromium": "cd browser-tests/playwright && npx playwright test --project=chromium",
    "browser:firefox": "cd browser-tests/playwright && npx playwright test --project=firefox",
    "browser:webkit": "cd browser-tests/playwright && npx playwright test --project=webkit",
    "browser:edge": "cd browser-tests/playwright && npx playwright test --project=edge",
    "browser:mobile": "cd browser-tests/playwright && npx playwright test --project=mobile-chrome --project=mobile-safari",
    "browser:tablet": "cd browser-tests/playwright && npx playwright test --project=tablet",
    "browser:visual": "cd browser-tests/playwright && npx playwright test visual.spec.ts",
    "browser:all": "cd browser-tests/playwright && node run-tests.js",
    "browser:ui": "cd browser-tests/playwright && npx playwright test --ui",
    "security:deps": "cd ../security-tests && node dependency-scan/run-dependency-scan.js",
    "security:static": "cd ../security-tests && node static-analysis/run-static-analysis.js",
    "security:zap": "cd ../security-tests && node zap/run-zap-scan.js",
    "security:pentest": "cd ../security-tests && node penetration-testing/security-tests.js",
    "security:all": "cd ../security-tests && node run-security-tests.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
